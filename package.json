  },
  "devDependencies": {
    "bluebird": "^3.7.2",
    "coveralls": "^3.1.0",
    "cross-spawn": "^7.0.3",
    "eslint": "^7.21.0",
    "intercept-stdout": "~0.1.2",
    "metrics": "^0.1.21",
    "mocha": "^8.3.0",
